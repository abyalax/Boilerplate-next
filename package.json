{
  "name": "boilerplate-nodejs-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "permissions:generate": "env-cmd -f .env npx tsx scripts/generate-route-permissions.ts",
    "permissions:watch": "env-cmd -f .env npx tsx scripts/generate-route-permissions.ts --watch",
    "dev": "concurrently \"pnpm run permissions:watch\" \"next dev --turbopack\"",
    "build": "pnpm permissions:generate && next build --turbopack",
    "start": "next start",
    "lint": "biome lint --write .",
    "format": "biome format --write .",
    "prepare": "husky",
    "extract:cv": "python3 python/extract_cv.py",
    "db:seed": "env-cmd -f .env npx tsx db/seed.ts",
    "db:seednew": "env-cmd -f .env npx tsx db/new.ts",
    "db:delete": "env-cmd -f .env npx tsx db/delete.ts",
    "db:query": "env-cmd -f .env tsx db/query.ts"
  },
  "dependencies": {
